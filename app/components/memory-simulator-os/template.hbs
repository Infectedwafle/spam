<div class="container">
	<div class="row">
		<div class="col-sm">
			<div class="row">
				Process Control Board
				<div>
					{{os.error}}
				</div>
			</div>
			<div class="row process-list">
				{{#each os.processControlList as |process|}}
					<div class="process">
						Process: {{process.id}} - Frame: {{process.frameNumber}}
					</div>
				{{/each}}
			</div>
		</div>
		<div class="col-sm">
			<div class="row">
				Current Page Table
			</div>
			<div class="row page-list">
				{{#each os.currentPageTable.pages as |page|}}
					<div class="page">
						{{#if (eq page.frameId null)}}
							Empty
						{{else}}
							Page: {{page.id}} - Frame: {{page.frameId}}
						{{/if}}
					</div>
				{{/each}}
			</div>	
		</div>
	</div>
</div>